{
  "name": "api-catalog",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "clean": "if [ -d dist ]; then rm -rf dist; fi",
    "build": "npm run clean && npx prisma generate && npx tsc",
    "start": "node --env-file .env dist/index.js --trace-warnings",
    "build:start": "npm run build && npm start",
    "gen": "node temp/routerGenerate.js",
    "db:up": "docker compose -f docker-compose-db.yml up -d --build",
    "db:down": "docker compose -f docker-compose-db.yml down --remove-orphans",
    "db:clean": "docker compose -f docker-compose-db.yml down -v --remove-orphans",
    "dc:bkp": "docker exec -t postgres pg_dumpall -c -U postgres > backup.sql",
    "dc:restore": "cat backup.sql | docker exec -i postgres psql -U postgres",
    "dc:up": "npm run db:up && docker compose up -d --build && docker compose logs -f api",
    "dc:down": "docker compose down --remove-orphans"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cron": "^3.5.0",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "glob": "^11.0.1",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^11.0.2",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "prisma": "^6.3.1",
    "socket.io": "^4.8.1",
    "ts-node": "^10.9.2",
    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.1",
    "@types/node-cron": "^3.0.11",
    "@types/ssh2": "^1.15.4",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/yamljs": "^0.2.34",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "prettier": "^3.4.2",
    "prisma-dbml-generator": "^0.12.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  }
}
